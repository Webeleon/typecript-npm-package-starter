{
  "name": "@webeleon/npm-package-starter-typescript",
  "version": "0.0.8",
  "description": "starter npm package using typescript",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "patch": "npm run build && npm version patch && npm publish",
    "format": "prettier --write '**/*.ts'",
    "build": "tsc",
    "generate-doc": "typedoc",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Webeleon/typecript-npm-package-starter"
  },
  "bugs": {
    "url": "https://github.com/Webeleon/typecript-npm-package-starter/issues"
  },
  "contributors": [
    {
      "name": "Julien Prugne",
      "email": "julien@webeleon.dev"
    }
  ],
  "keywords": [
    "project starter",
    "webeleon",
    "npm",
    "typescript"
  ],
  "author": "Julien Prugne<juliene@webeleon.dev>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/jest": "27.0.1",
    "@types/sinon": "10.0.2",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "sinon": "11.1.2",
    "standard-version": "9.3.1",
    "ts-jest": "27.0.4",
    "ts-node": "10.2.0",
    "typedoc": "0.21.5",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "node-fetch": "2.6.1",
    "query-string": "7.0.1"
  },
  "lint-staged": {
    "*.{ts}": "prettier --write"
  }
}
